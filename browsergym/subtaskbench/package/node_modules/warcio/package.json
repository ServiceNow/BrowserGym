{
  "name": "warcio",
  "version": "2.2.1",
  "keywords": [
    "WARC",
    "web archiving"
  ],
  "homepage": "https://github.com/webrecorder/warcio.js#readme",
  "bugs": {
    "url": "https://github.com/webrecorder/warcio.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webrecorder/warcio.js.git"
  },
  "license": "Apache-2.0",
  "author": "Webrecorder Software, Ilya Kreymer",
  "sideEffects": [
    "./src/polyfills.cjs"
  ],
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./utils": {
      "require": "./dist/utils.cjs",
      "default": "./dist/utils.js"
    },
    "./node": {
      "require": "./dist/node/index.cjs",
      "default": "./dist/node/index.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "bin": "./dist/cli.js",
  "files": [
    "/src",
    "/dist"
  ],
  "scripts": {
    "build": "tsup && tsup --define.browser='true'",
    "lint": "eslint 'src/**/*.ts'",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --collectCoverage"
  },
  "dependencies": {
    "base32-encode": "^2.0.0",
    "hash-wasm": "^4.9.0",
    "pako": "^1.0.11",
    "tempy": "^3.1.0",
    "uuid-random": "^1.3.2",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/eslint": "^7.29.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/pako": "^1.0.0",
    "@types/stream-buffers": "^3.0.4",
    "@types/yargs": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "cross-fetch": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-deprecation": "^1.5.0",
    "jest": "^29.6.2",
    "jest-expect-message": "^1.1.3",
    "stream-buffers": "^3.0.2",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
