{
  "name": "webreplay",
  "version": "0.0.1",
  "private": true,
  "description": "This is the ui automation webreplay for orby extension",
  "scripts": {
    "evaluate": "ts-node src/index.ts evaluate",
    "serve": "ts-node src/index.ts serve",
    "serve_standalone": "ts-node src/index.ts serve_standalone",
    "record": "ts-node src/index.ts record",
    "upload": "ts-node src/index.ts upload",
    "unzip": "ts-node src/index.ts unzip",
    "lint": "eslint src",
    "test": "jest",
    "copy": "./build-tools/copy.sh",
    "generate-test-warc": "ts-node src/tools/generate-test-warc.ts",
    "build": "rimraf dist && cross-env NODE_OPTIONS='--max-old-space-size=16384' webpack --config webpack.config.js",
    "build:prod": "rimraf dist && cross-env NODE_OPTIONS='--max-old-space-size=16384' webpack --config webpack.prod.js",
    "build:dev": "rimraf dist && cross-env NODE_OPTIONS='--max-old-space-size=16384' webpack --config webpack.dev.js",
    "start": "webpack serve --config webpack.dev.js"
  },
  "dependencies": {
    "@google-cloud/storage": "7.12.0",
    "@googleapis/sheets": "9.0.0",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@protobuf-ts/protoc": "^2.9.5",
    "@sentry/browser": "^9.5.0",
    "adm-zip": "0.5.12",
    "chalk": "4.1.2",
    "commander": "12.0.0",
    "diff": "7.0.0",
    "glob": "11.0.1",
    "playwright": "1.48.2",
    "protobufjs": "^7.2.6",
    "warcio": "2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@jest/globals": "29.7.0",
    "@types/adm-zip": "0.5.5",
    "@types/diff": "7.0.1",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "12.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.5",
    "dotenv-webpack": "8.1.0",
    "jest": "29.7.0",
    "rimraf": "5.0.5",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "webpack": "5.91.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.4",
    "webpack-merge": "5.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
